{
  "server": true,
  "bootstrap_expect": 1,
  "bind_addr": "0.0.0.0",
  "data_dir": "${CONSUL_DATA_DIR}",
  "node_name": "${CONSUL_NODE_NAME}",
  "log_level": "INFO",
  "enable_debug": true,
  "leave_on_terminate": false,
  "skip_leave_on_interrupt": true,
  "encrypt": "f502FprfDugOXiWqZUJpyAwgeXH+6qs6VNFjPxX8TgU=",
  "client_addr": "0.0.0.0",
  "ui_config": {
    "enabled": true
  },
  "addresses": {
    "https": "0.0.0.0"
  },
  "ports": {
    "http": -1,
    "https": 8501
  },
  "tls": {
    "defaults": {
      "key_file": "${KEYSTORE_HOME}/consul.remal.com.key",
      "cert_file": "${KEYSTORE_HOME}/consul.remal.com.crt",
      "ca_file": "${KEYSTORE_HOME}/ca.crt",
      "verify_incoming": true,
      "verify_outgoing": true
    },
    "internal_rpc": {
      "verify_server_hostname": true
    }
  }
}
